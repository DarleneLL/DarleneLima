<h1>Recebe 2 números e mostra o maior</h1>
    <script>
        let x = prompt("Digite o primeiro número:")
        let y = prompt("Digite o segundo número")
        
        comparaMaiorIgual(x,y)

        function comparaMaiorIgual(x,y){
            if(x > y){
                alert("O número " + x + " é maior que o número " + y)
            } else if(y > x){
                alert("O número " + y + " é maior que o número " + x)
            } else{
                alert("O número " + x + " é igual ao número " + y)
            }
        }
    </script>
